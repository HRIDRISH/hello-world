//sum of two numbers
#include <stdio.h>
int main() {
    int num1, num2, sum;

    // user jonok input lobor babey jonuwa hol

    printf("Enter the first number: ");
    scanf("%d", &num1);

    printf("Enter the second number: ");
    scanf("%d", &num2);

    // sum tu calculate kora hol
    sum = num1 + num2;

    // result tu dekhuwa hol
    printf("The sum of %d and %d is %d\n", num1, num2, sum);

    return 0;
}
